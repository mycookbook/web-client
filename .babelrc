{
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "commonjs",
          "useBuiltIns": "entry",
          "corejs": 3
        }],
        "@vue/babel-preset-jsx"
      ],
      "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-syntax-dynamic-import",
        "babel-plugin-transform-vue-jsx"
      ]
    }
  },
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "useBuiltIns": "entry",
      "corejs": 3
    }],
    "@vue/babel-preset-jsx"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import"
  ]
}